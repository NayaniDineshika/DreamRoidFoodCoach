<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/photo_1550807014_1236e91b92d4"
    tools:context=".FoodHome">

    <Button
        android:id="@+id/button27"
        android:layout_width="213dp"
        android:layout_height="100dp"
        android:layout_marginTop="12dp"
        android:rotationY="-17"
        android:text="BMI Calculator"
        android:textAllCaps="false"
        android:textSize="24sp"
        android:textStyle="bold"
        app:backgroundTint="#A44CAF50"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.805"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button28" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar9"
        android:layout_width="409dp"
        android:layout_height="61dp"
        android:background="#424242"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="282dp"
        android:layout_height="61dp"
        android:text="@string/foodcoah"
        android:textAlignment="center"
        android:textColor="#F6F3F3"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/button34"
        app:layout_constraintHorizontal_bias="0.733"
        app:layout_constraintStart_toStartOf="@+id/toolbar9"
        app:layout_constraintTop_toTopOf="@+id/toolbar9" />

    <Button
        android:id="@+id/button25"
        android:layout_width="213dp"
        android:layout_height="100dp"
        android:layout_marginTop="12dp"
        android:rotationY="17"
        android:text="Food Recepi"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:backgroundTint="#A44CAF50"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.209"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar9" />

    <Button
        android:id="@+id/button26"
        android:layout_width="213dp"
        android:layout_height="100dp"
        android:layout_marginTop="12dp"
        android:rotationY="-17"
        android:text="Meal Plan"
        android:textAllCaps="false"
        android:textSize="24sp"
        android:textStyle="bold"
        app:backgroundTint="#A44CAF50"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.661"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button25" />

    <Button
        android:id="@+id/button28"
        android:layout_width="213dp"
        android:layout_height="100dp"
        android:layout_marginTop="16dp"
        android:rotationY="17"
        android:text="Fitness"
        android:textAllCaps="false"
        android:textSize="24sp"
        android:textStyle="bold"
        app:backgroundTint="#A44CAF50"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.237"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button26" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar10"
        android:layout_width="409dp"
        android:layout_height="67dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="50dp"
        android:background="#424242"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button27" />

    <Button
        android:id="@+id/button29"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_marginEnd="52dp"
        android:background="#00FFFFFF"
        android:text="Button"
        app:icon="@drawable/ic_baseline_home_24"
        app:layout_constraintEnd_toStartOf="@+id/button30"
        app:layout_constraintTop_toTopOf="@+id/toolbar10"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/button30"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_marginTop="4dp"
        android:background="#00FFFFFF"
        android:text="Button"
        app:icon="@drawable/ic_baseline_shopping_bag_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbar10"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/button31"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="4dp"
        android:background="#00FFFFFF"
        android:text="Button"
        app:icon="@drawable/ic_baseline_account_circle_24"
        app:layout_constraintStart_toEndOf="@+id/button30"
        app:layout_constraintTop_toTopOf="@+id/toolbar10"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/button34"
        android:layout_width="79dp"
        android:layout_height="61dp"
        android:background="#00FFFFFF"
        android:text="Button"
        app:icon="@drawable/ic_baseline_settings_24"
        app:iconSize="45dp"
        app:layout_constraintLeft_toRightOf="@id/textView6"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>